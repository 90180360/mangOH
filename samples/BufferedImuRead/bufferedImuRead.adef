sandboxed: false
version: 0.1
start: manual

executables:
{
    bufferedImuRead = ( bufferedImuReadComponent )
}

processes:
{
    run:
    {
        ( bufferedImuRead )
    }
    envVars:
    {
        // This is required so that the bundled libraries are visible to the non-sandboxed app
        LD_LIBRARY_PATH = /legato/systems/current/apps/bufferedImuRead/read-only/usr/lib
    }
    faultAction: stopApp
}
